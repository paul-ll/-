.tab-content-item{
	border-bottom: 1px dashed #e5e5e5;
}
.favorite-film-item {
    display: flex;
    padding: 40rpx;
    
}


.favorite-film-cover {
    width:60px!important;
    height:80px!important;
    box-shadow: 10rpx 10rpx 30rpx rgba(0, 0, 0, .2);
    display:block;
}

.favorite-film-info {
    width: 100%;
}

.favorite-film-txt {
    font-family: PingFangSC-Regular;
	font-size: 12px;
	color: #888888;
	line-height: 12px;
}

.favorite-film-title {
    font-family: PingFangSC-Regular;
	font-size: 16px;
	color: #000000;
	line-height: 16px;

}

.favorite-person-item {
    box-sizing: border-box;
    padding: 40rpx;
    margin-bottom: 60rpx;
    background-color: #fff;
    border: 1px solid #e1e1e1;
    border-radius: 20rpx;
    box-shadow: 0 10rpx 30rpx rgba(0, 0, 0, .1);
}

.favorite-person-intro {
    display: flex;
    justify-content: space-between;
}

.favorite-person-info {
    width: 100%;
}

.favorite-person-time {
    margin-bottom: 20rpx;
    font-size: 36rpx;
    text-align: center;
    color: #ccc;
}

.favorite-person-cover {
  
    margin-bottom: 30rpx;
  
    box-shadow: 0 20rpx 60rpx rgba(0, 0, 0, .1);
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

.favorite-person-txt {
    margin-bottom: 10rpx;
    color: #999;
}

.favorite-person-name {
    font-size: 40rpx;
    color: #666;
}
.examine_info_electronic,.event_pay,.hold_event,.ticket_check{
	padding:15px;
	padding-bottom:15px;
	border-bottom:1px dashed #e5e5e5;
}
.check_title{
	font-family: PingFangSC-Regular;
	font-size: 14px;
	color: #000000;
	line-height: 14px;
	margin-top:10px;
}
.check_text{
	font-family: PingFangSC-Regular;
	font-size: 12px;
	color: #676767;
	line-height: 12px;
	margin-top:10px;
}
.check_text text{
	font-family: PingFangSC-Regular;
	font-size: 14px;
	color: #000000;
	line-height: 12px;
	margin-left:5px;
}
.check_img{
	width:150px;
	height:150px;
	display:block;
	margin:0 auto;
}
.hold_event{
	display:flex;
	justify-content: space-between;
}
.hold_name{
	font-family: PingFangSC-Regular;
	font-size: 14px;
	color: #000000;
	line-height: 14px;
}
.hold_name text{
	display:block;
	font-family: PingFangSC-Regular;
	font-size: 12px;
	color: #4A4A4A;
	line-height: 12px;
	margin-top:10px;
}
.hold_img{
	width:36px;
	height:36px;
	display:block;
}
.pay_title{
	font-family: PingFangSC-Regular;
	font-size: 14px;
	color: #000000;
	line-height: 14px;
	margin-top:10px;
}
.pay_num,.pay_mon{
	font-family: PingFangSC-Regular;
	font-size: 12px;
	color: #4A4A4A;
	line-height: 12px;
	margin-top:10px;
}
.examine_info_electronic text{
	display:block;
	font-family: PingFangSC-Regular;
	font-size: 12px;
	color: #888888;
}

